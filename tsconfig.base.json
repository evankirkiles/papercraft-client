{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "preserve",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "jsx": "preserve"
  }
}
